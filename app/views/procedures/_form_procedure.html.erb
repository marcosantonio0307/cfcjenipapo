<div class="form-center">
	<div class="form-group">
			<%= label_tag :aluno %>
			<%= text_field_tag :studant, @procedure.studant.name, class: 'form-control' %>
	</div>

	<%= form_for @procedure do |f| %>

		<div class="form-group">
			<%= f.label 'Numero do Processo' %>
			<%= f.text_field :number, class: 'form-control' %>
		</div>

		<div class="form-group">
			<%= f.label 'Vencimento do Processo' %>
			<%= f.date_field :due_date, class: 'form-control' %>
		</div>

		<div class="form-group">
			<%= f.label :categoria %><br>
			<%= f.select :category, ['A', 'B', 'AB'] %>
		</div>

		<div class="form-group">
			<%= f.label :situação %><br>
			<%= f.select :status, ['Aberto', 'Concluido'] %>
		</div>

		<div class="form-group">
			<%= f.submit :salvar, class: 'btn btn-success' %>
		</div>
	<% end %>
</div>